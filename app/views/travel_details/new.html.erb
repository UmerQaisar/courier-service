<%= render 'form' %>


<!-- Jquery Library: Geocomplete  -->
<script type="text/javascript" src="https://unpkg.com/geocomplete@1.7.0/jquery.geocomplete.js"></script>

<script>

    $('#travel_detail_from').geocomplete({
        details: ".details_from",
        detailsAttribute: "data-geo"
    });

    $('#travel_detail_to').geocomplete({
        details: ".details_to",
        detailsAttribute: "data-geo"
    });

</script>


