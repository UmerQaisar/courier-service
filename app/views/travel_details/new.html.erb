
<%= render 'form' %>

<script>

  let autocomplete;

  function initMap(){

      const input_to = document.getElementById('travel_detail_to')
      const input_from = document.getElementById('travel_detail_from')

      const options = {
          componentRestrictions: { country: "PK" },
          fields: ["address_components", "geometry", "icon", "name"],
          strictBounds: false,
          types: ["establishment"],
      };

      autocomplete = new google.maps.places.Autocomplete(input_to, options);
      autocomplete = new google.maps.places.Autocomplete(input_from, options);
  }

</script>

<script
        src="https://maps.googleapis.com/maps/api/js?key=<%=ENV["GOOGLE_API_KEY"]%>&libraries=places&callback=initMap" async defer >
</script>

